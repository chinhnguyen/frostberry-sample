{
  "name": "frostberry-sample",
  "version": "1.0.0",
  "description": "An example app for Frostberry.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "deploy": "npm run build && echo \"--- Deploying to S3 ---\" && aws s3 sync --delete dist s3://frostberry-sample",
    "start": "webpack-dev-server --mode development --open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-sdk": "export JS_POST_PROCESS_FILE=\"js-beautify -r -f\" && openapi-generator generate -i sdk/CompaniesAPI.yaml -o sdk/lib -g javascript --additional-properties useES6=true,usePromises=true,projectName=frostberry-api,projectDescription=\"Frostberry JS ApiClient\",projectVersion=\"0.1.1\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chinhnguyen/frostberry-sample.git"
  },
  "author": "Chinh Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chinhnguyen/frostberry-sample/issues"
  },
  "homepage": "https://github.com/chinhnguyen/frostberry-sample#readme",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@uirouter/angularjs": "^1.0.21",
    "angular": "^1.7.6",
    "angular-animate": "^1.7.6",
    "angular-aria": "^1.7.6",
    "angular-material": "^1.1.12",
    "angular-messages": "^1.7.6",
    "angular-route": "^1.7.6",
    "babel-loader": "^8.0.5",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "bourbon": "^5.1.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "frostberry-api": "file:sdk/lib",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.11",
    "ng-table": "^3.0.1",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
